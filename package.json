{
  "name": "React-antd-mobx",
  "version": "0.1.0",
  "description": "React-antd-mobx",
  "main": "build/main.js",
  "scripts": {
    "start": "BABEL_ENV=development webpack-dev-server -w --open --env=development",
    "vendor": "webpack --progress --env=dll",
    "_build": "BABEL_ENV=production webpack --progress --env=production",
    "build": "npm run vendor && npm run _build",
    "build:daily": "npm run vendor && BABEL_ENV=production webpack --env=daily",
    "container:add": "node script/add.js",
    "prettier": "prettier src/**/*.js src/**/*.jsx --write",
    "test": "jest ."
  },
  "author": {
    "name": "mingyang.chen"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn test",
      "yarn prettier",
      "git add"
    ]
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "autoprefixer": "^8.2.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "dx-mock": "^0.1.0",
    "escodegen": "^1.9.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-react": "^7.7.0",
    "esprima": "^4.0.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^4.2.0",
    "jest": "^24.5.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-less": "^2.0.0",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.0.1",
    "style-loader": "^0.20.3",
    "svg-inline-loader": "^0.8.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.1.0",
    "url-loader": "^1.0.1",
    "webpack": "4.5",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "antd": "^3.12.3",
    "babel-polyfill": "^6.26.0",
    "bundle-loader": "^0.5.6",
    "mobx": "5.9.0",
    "mobx-react": "5.4.3",
    "moment": "^2.29.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.2.2"
  }
}
